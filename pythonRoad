#square root code

num = float (input("enter the number: "))
sqrt_num = num**.5
print()
print("square root of entered number is ")
print(sqrt_num)

num = float (input("enter the number: "))
sqrt_num = num**.5
print()
print("square root of entered %.3f is %.3f'" %(num , sqrt_num))
